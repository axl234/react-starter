{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "react前端开发脚手架",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config './config/webpack.config.js' --open --mode development",
    "build": "cross-env BUILD_ENV=VERSION webpack --config './config/webpack.config.js' --mode production --progress --display-modules && npm run tar",
    "build-cdn": "cross-env BUILD_ENV=CDN webpack --config './config/webpack.config.js' --mode production --progress --display-modules && npm run tar",
    "fix": "eslint --ext .js,.ts,.jsx,.tsx ./src --fix",
    "tar": "node ./scripts/tar.js",
    "build-mock": "node ./scripts/build-mock.js "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rebareba/react-starter.git"
  },
  "keywords": [
    "demo"
  ],
  "author": "长风",
  "license": "ISC",
  "devDependencies": {
